    <script>
        // Fonction pour afficher/masquer la barre de recherche
        function toggleSearch() {
            var searchContainer = document.querySelector('.search-container');
            searchContainer.classList.toggle('active');
        }

        // Fonction pour g√©rer la recherche et redirection
        function searchAndRedirect(event) {
            event.preventDefault(); // Emp√™che le rechargement de la page
            var query = document.querySelector('.search-box').value.toLowerCase();

            // Liste des termes et des liens associ√©s
            var links = {
                "kibriv": "https://kibriv.com/",
                "papadustream": "https://papadustream.mov/",
                "sokrostream": "https://sokrostream.tube/streaming-complet-3/",
                "plutotv": "https://pluto.tv/fr/live-tv/66e1978babec540008ce41ac",
                "manga": "https://w14.french-manga.net/",
                "rakuten": "https://www.rakuten.tv/fr"
            };

            // Si la recherche correspond √† une cl√©, rediriger vers le lien associ√©
            if (links[query]) {
                window.location.href = links[query];
            } else {
                alert("Aucun lien trouv√© pour cette recherche.");
            }
        }
    </script>
</head>
